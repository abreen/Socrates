<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="io.breen.socrates.view.setup.SetupView">
  <grid id="27dc6" binding="rootPanel" layout-manager="CardLayout" hgap="0" vgap="0" show="9f86b">
    <constraints>
      <xy x="20" y="20" width="509" height="432"/>
    </constraints>
    <properties>
      <minimumSize width="300" height="400"/>
    </properties>
    <border type="none"/>
    <children>
      <grid id="9f86b" binding="openCriteriaPanel" layout-manager="BorderLayout" hgap="0" vgap="10">
        <constraints>
          <card name="CriteriaCard"/>
        </constraints>
        <properties/>
        <border type="empty">
          <size top="20" left="20" bottom="20" right="20"/>
        </border>
        <children>
          <component id="59a55" class="javax.swing.JLabel">
            <constraints border-constraint="North"/>
            <properties>
              <font name="Dialog" size="14" style="1"/>
              <text value="Open a criteria file"/>
            </properties>
          </component>
          <component id="6fbe5" class="javax.swing.JTextArea" default-binding="true">
            <constraints border-constraint="Center"/>
            <properties>
              <editable value="false"/>
              <font name="Dialog"/>
              <lineWrap value="true"/>
              <opaque value="false"/>
              <text value="A criteria file is a special file prepared by a grading coordinator that contains the expected files in a student submission and how to grade them.&#10;&#10;Without a criteria file, you cannot use Socrates to grade submissions. If you do not have a criteria file, contact the grading coordinator for your course."/>
              <wrapStyleWord value="true"/>
            </properties>
          </component>
          <grid id="60348" layout-manager="FlowLayout" hgap="5" vgap="5" flow-align="2">
            <constraints border-constraint="South"/>
            <properties/>
            <border type="none"/>
            <children>
              <component id="17c62" class="javax.swing.JButton" binding="quitButton">
                <constraints/>
                <properties>
                  <text value="Quit"/>
                </properties>
              </component>
              <component id="56b53" class="javax.swing.JButton" binding="openButton">
                <constraints/>
                <properties>
                  <text value="Open Criteria..."/>
                </properties>
              </component>
            </children>
          </grid>
        </children>
      </grid>
      <grid id="bd396" binding="selectSubmissionsPanel" layout-manager="BorderLayout" hgap="0" vgap="10">
        <constraints>
          <card name="SubmissionsCard"/>
        </constraints>
        <properties/>
        <border type="empty">
          <size top="20" left="20" bottom="20" right="20"/>
        </border>
        <children>
          <component id="55dd6" class="javax.swing.JLabel">
            <constraints border-constraint="North"/>
            <properties>
              <font name="Dialog" size="14" style="1"/>
              <text value="Select student submissions"/>
            </properties>
          </component>
          <component id="ceb07" class="javax.swing.JTextArea" default-binding="true">
            <constraints border-constraint="Center"/>
            <properties>
              <editable value="false"/>
              <font name="Dialog"/>
              <lineWrap value="true"/>
              <opaque value="false"/>
              <text value="Now select the student submissions you would like to grade. You must select at least one, but you may add more once you have started by using the File â†’ Add Submissions... option.&#10;&#10;Each submission you select must be a folder containing the files submitted by one student. If you were sent a compressed archive (for example, a ZIP file) containing the submissions, decompress the archive before continuing.&#10;&#10;If you are using receipts, make sure the receipt for each student is at the top level of each submission folder you select."/>
              <wrapStyleWord value="true"/>
            </properties>
          </component>
          <grid id="837aa" layout-manager="FlowLayout" hgap="5" vgap="5" flow-align="2">
            <constraints border-constraint="South"/>
            <properties/>
            <border type="none"/>
            <children>
              <component id="4ffc7" class="javax.swing.JButton" binding="selectButton">
                <constraints/>
                <properties>
                  <label value="Select Submissions..."/>
                  <text value="Select Submissions..."/>
                </properties>
              </component>
            </children>
          </grid>
        </children>
      </grid>
    </children>
  </grid>
</form>
